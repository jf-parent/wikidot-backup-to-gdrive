bot_diary:
  enable_auto_bot_diary: false
  enable_auto_bot_diary_component_screenshot: true
  enable_bot_diary: true
  streamlogger: true
  filelogger: true
browser:
  maximize_window: false
  window_height: 725
  window_width: 1650
  window_x_position: 0
  window_y_position: 0
database:
  sqlalchemy.url: !!python/unicode 'sqlite:///database.db' #CHANGEME
saucelabs:
  username: ''
  key: ''
browserstack:
  username: ''
  key: ''
ec2:
  wait_after_instance_launched: 30
  wait_until_system_and_instance_check_performed: true
brome:
  script_folder_name: 'scripts'
  script_test_prefix: 'task_'
grid_runner:
  kill_selenium_server: true
  max_running_time: 7200
  selenium_hub_config: ''
  selenium_server_command: ''
  selenium_server_ip: localhost
  selenium_server_jar_path: ''
  selenium_server_port: 4444
  start_selenium_server: true
highlight:
  highlight_on_assertion_failure: false
  highlight_on_assertion_success: false
  highlight_when_element_is_clicked: true
  highlight_when_element_is_visible: true
  highlight_when_element_receive_keys: true
  style_on_assertion_failure: 'background: red; border: 2px solid black;'
  style_on_assertion_success: 'background: green; border: 2px solid black;'
  style_when_element_is_clicked: 'background: yellow; border: 2px solid red;'
  style_when_element_is_visible: 'background: purple; border: 2px solid black;'
  style_when_element_receive_keys: 'background: yellow; border: 2px solid red;'
  use_highlight: true
logger_runner:
  filelogger: true
  format: !!python/unicode "[%(batchid)s]\e[32m%(message)s\e[0m"
  level: INFO
  streamlogger: true
logger_test:
  filelogger: true
  format: !!python/unicode "[%(batchid)s]\e[34m(%(testname)s)\e[0m:\e[32m%(message)s\e\
    [0m"
  level: INFO
  streamlogger: true
project:
  test_batch_result_path: '' #CHANGME
  url: '' #CHANGEME
  username: '' #CHANGEME
  password: '' #CHANGEME
  folder_id: '' #CHANGEME
mailer:
  from: '' #CHANGEME
  to: '' #CHANGEME
  smtp_server: '' #CHANGEME
proxy_driver:
  default_timeout: 5
  intercept_javascript_error: false
  raise_exception: true
  take_screenshot_on_assertion_failure: false
  take_screenshot_on_assertion_success: false
  validate_css_selector: true
  validate_xpath_selector: true
  wait_until_not_present_before_assert_not_present: false
  wait_until_not_visible_before_assert_not_visible: false
  wait_until_present_before_assert_present: false
  wait_until_present_before_find: false
  wait_until_visible_before_assert_visible: false
  wait_until_visible_before_find: false
runner:
  cache_screenshot: true
  embed_on_assertion_failure: false
  embed_on_assertion_success: false
  embed_on_test_crash: true
  play_sound_on_assertion_failure: false
  play_sound_on_assertion_success: false
  play_sound_on_ipython_embed: false
  play_sound_on_pdb: false
  play_sound_on_test_crash: false
  play_sound_on_test_finished: false
  sound_on_assertion_failure: '{testid} failed'
  sound_on_assertion_success: '{testid} succeeded'
  sound_on_ipython_embed: Attention required
  sound_on_pdb: Attention required
  sound_on_test_crash: Crash
  sound_on_test_finished: Test finished
webserver:
  ASSETS_DEBUG: true
  CACHE_TYPE: simple
  CLOSED_REGISTRATION: true
  DEBUG: false
  DEBUG_TB_ENABLED: false
  DEBUG_TB_INTERCEPT_REDIRECTS: false
  HOST: localhost
  PORT: 5000
  REGISTRATION_TOKEN: '' #CHANGEME
  SECRET_KEY: '' #CHANGEME
  SHOW_VIDEO_CAPTURE: true
  SHOW_NETWORK_CAPTURE: true
  SHOW_TEST_INSTANCES: true
  filelogger: true
  level: INFO
  streamlogger: true
